{
  "apiVersion": "v1",
  "kind": "BootstrapService",
  "metadata": {
    "namespace": "",
    "name": "",
    "labels": null,
    "annotations": null
  },
  "spec": {
    "apiVersion": "v1",
    "kind": "Deployment",
    "metadata": {
      "name": "frontend"
    },
    "spec": {
      "containers": [
        {
          "name": "app",
          "image": "flipt/flipt:v1.27.0",
          "resources": {
            "requests": {
              "memory": "64Mi",
              "cpu": "250m"
            },
            "limits": {
              "memory": "128Mi",
              "cpu": "500m"
            }
          }
        }
      ]
    }
  }
}
